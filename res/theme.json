    "menu_bar.#file_menu_items.#save_as": {
        "prototype": "#smaller_font"
    },
    "menu_bar.#edit_menu_items.#undo": {
        "prototype": "#smaller_font"
    },
    "menu_bar.#edit_menu_items.#redo": {
        "prototype": "#smaller_font"
    },
    "menu_bar.#view_menu_items.#theme": {
        "prototype": "#smaller_font"
    },
    "menu_bar.#view_menu_items.#workspace": {
        "prototype": "#smaller_font"
    },
    "menu_bar.#view_menu_items.#info": {
        "prototype": "#smaller_font"
