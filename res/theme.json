        "font": {
            "name": "caviar_dreams",
            "size": "15",
            "bold": "1",
            "regular_path": "res\\fonts\\CaviarDreams.ttf",
            "bold_path": "res\\fonts\\CaviarDreams_Bold.ttf"
        }
    },
    "#brush_button": {
        "images": {
            "normal_image": {
                "path": "res\\brush_button.png"
            }
        }
