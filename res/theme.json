{
    "default": {
        "font": {
            "name": "caviar_dreams",
            "size": "15",
            "bold": "1",
            "regular_path": "res\\fonts\\CaviarDreams.ttf",
            "bold_path": "res\\fonts\\CaviarDreams_Bold.ttf"
        }
    },
    "menu_bar": {
        "misc": {
            "shape": "rectangle",
            "shadow_width": "0",
